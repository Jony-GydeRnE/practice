<%- include("includes/head.ejs") %>
<link rel="stylesheet" href="css/userList.css">
</head>
<body>    
    <%- include("includes/nav.ejs") %>
    <main>
        <h1><%= info %></h1>
        <ul>
            <% if(users.length > 0){ %>
                <% users.forEach(user => { %>
                    <li>
                       <%= user %>
                    </li>
                <% }) %>
            <%} else { %>
                <li>"Ther are no Users"</li>
            <% } %>
        </ul>        
    </main>
</body>
</html>